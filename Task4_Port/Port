import java.util.ArrayList;

public class Port {
    private int capacityOfPort;
    private int numberOfDockings;
    private int currentNumberOfContainersInPort;
    public ArrayList<Ship> listOfShips=new ArrayList<Ship>();

    public Port() {
    }

    public Port(int capacityOfPort, int numberOfDockings,int currentNumberOfContainersInPort) {
        this.capacityOfPort = capacityOfPort;
        this.numberOfDockings=numberOfDockings;
        this.currentNumberOfContainersInPort=currentNumberOfContainersInPort;
    }
    public ArrayList<Ship> getListOfShips() {
        return listOfShips;
    }

    public void setListOfShips(ArrayList<Ship> listOfShips) {
        this.listOfShips = listOfShips;
    }

    public int getCurrentNumberOfContainersInPort() {
        return currentNumberOfContainersInPort;
    }

    public void setCurrentNumberOfContainersInPort(int currentNumberOfContainersInPort) {
        this.currentNumberOfContainersInPort = currentNumberOfContainersInPort;
    }

    public int getNumberOfDockings() {
        return numberOfDockings;
    }

    public void setNumberOfDockings(int numberOfDockings) {
        this.numberOfDockings = numberOfDockings;
    }

    public int getCapacityOfPort() {
        return capacityOfPort;
    }

    public void setCapacityOfPort(int capacityOfPort) {
        this.capacityOfPort = capacityOfPort;
    }
}
